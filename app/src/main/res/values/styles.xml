        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">@dimen/dimen_12dp</item>
        <item name="android:paddingRight">@dimen/dimen_12dp</item>
        <item name="android:minWidth">@dimen/dimen_24dp</item>
        <item name="android:minHeight">@dimen/dimen_24dp</item>
    </style>

    <style name="ProgressBarStyle">
        <item name="colorAccent">#fff</item>
    </style>

